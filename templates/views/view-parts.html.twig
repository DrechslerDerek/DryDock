<turbo-frame id="tf-view">
    <div class="container-fluid">
        <div class="row justify-content-center">
            <div class="col-11">
                <h1>View Parts</h1>
                <p>View parts created by you or others, you can only edit or delete parts you've created</p>
            </div>
        </div>
        <div class="row justify-content-center mt-5">
            <div class="col-12">
                {% include 'components/parts/parts-filters.html.twig' %}
            </div>
        </div>
        <div class="row justify-content-center">
            <div class="col-12 table-responsive mb-5 pb-5" {{ stimulus_controller('parts-table') }}>
                {% include 'components/parts/parts-table.html.twig' %}
            </div>
        </div>
    </div>
</turbo-frame>
