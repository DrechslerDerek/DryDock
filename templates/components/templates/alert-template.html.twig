<template id="alertTemplate">
    <div class="alert-bg p-3 rounded-3 mb-3">
        <span class="h5">Part Created</span><br>
        <span class="fs-6">Part: <span id="partName"></span></span><br>
        <span class="fs-6">Created by: <span id="partCreatedName"></span></span><br>
        <span class="fs-6">Created at: <span id="partCreatedTime"></span></span><br>
        <button class="btn btn-danger btn-sm mt-2" {{ stimulus_action('alert','deleteAlert') }}><i class="bi bi-trash2-fill me-2"></i>Delete</button>
        <button class="btn btn-outline-light btn-sm disabled mt-2"><i class="bi bi-eye-fill me-2"></i>View</button>
    </div>
</template>
