<!DOCTYPE html>
<html lang="en-us">
{% block head %}
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>{% block title %}Dry Dock{% endblock %}</title>
        <link rel="icon" href="{{ asset('build/images/rocket-takeoff.svg') }}">
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
{% endblock %}
    <body>
        {% block body %}{% endblock %}
    </body>
</html>
